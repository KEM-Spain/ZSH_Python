_BOX_LINE_WEIGHT=''
+/usr/local/lib/MSG.zsh:9> _BOX_LINE_WEIGHT='' 
_MSG_KEY=n
+/usr/local/lib/MSG.zsh:10> _MSG_KEY=n 
_PROC_MSG=false
+/usr/local/lib/MSG.zsh:11> _PROC_MSG=false 
