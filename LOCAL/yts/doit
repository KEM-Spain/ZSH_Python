#!/usr/bin/zsh
RESET="\033[m"
CYAN_FG="\033[36m"
GREEN_FG="\033[32m"
MAGENTA_FG="\033[35m"
RED_FG="\033[31m"
WHITE_FG="\033[37m"
YELLOW_FG="\033[33m"

RED_ON_GREY="\033[0m\033[0;1;31;100m"
GREEN_ON_GREY="\033[0m\033[0;1;32;100m"
YELLOW_ON_GREY="\033[0m\033[0;1;33;100m"
MAGENTA_ON_GREY="\033[0m\033[0;1;35;100m"
CYAN_ON_GREY="\033[0m\033[0;1;36;100m"
WHITE_ON_GREY="\033[0m\033[0;1;37;100m"

echo "${WHITE_ON_GREY}TEST TEST TEST${RESET}"
echo "${GREEN_ON_GREY}TEST TEST TEST${RESET}"
echo "${YELLOW_ON_GREY}TEST TEST TEST${RESET}"
echo "${MAGENTA_ON_GREY}TEST TEST TEST${RESET}"
echo "${CYAN_ON_GREY}TEST TEST TEST${RESET}"
echo "${RED_ON_GREY}TEST TEST TEST${RESET}"
