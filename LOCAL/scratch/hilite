list_search_mark_targets () {
	local PAGE=${1}
	local HDR_OFFSET=${#_LIST_HEADER}
	local R

	[[ ${_DEBUG} -ge ${_LIST_LIB_DBG} ]] && dbg "${functrace[1]} called ${0}:${LINENO}: ARGC:${#@}"

	# Highlight search targets
	for R in ${_TARGETS};do
		IFS=":" read _TARGET_NDX _TARGET_CURSOR _TARGET_PAGE TNDX <<<${R}
		_LIST_NDX=${_TARGET_NDX}
		if [[ ${_TARGET_PAGE} == ${PAGE} ]];then
			list_item high ${_LIST_LINE_ITEM} $(( _TARGET_NDX + HDR_OFFSET - 1 )) 0
		fi
	done 
}
